<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!--Replace LineageOS one with AOSP one-->
    <remove-project name="LineageOS/android_hardware_qcom_audio"/>
    <project path="hardware/qcom/audio" name="platform/hardware/qcom/audio" remote="aosp"
             groups="qcom,qcom_audio,pdk-qcom"/>
    <!--Replace LineageOS one with AOSP one-->
    <remove-project name="LineageOS/android_hardware_qcom_bootctrl"/>
    <project path="hardware/qcom/bootctrl" name="platform/hardware/qcom/bootctrl" remote="aosp" groups="pdk-qcom"/>
    <!--Replace LineageOS one with AOSP one-->
    <remove-project name="LineageOS/android_hardware_nxp_nfc"/>
    <project path="hardware/nxp/nfc" name="platform/hardware/nxp/nfc" remote="aosp" groups="pdk"/>
</manifest>
